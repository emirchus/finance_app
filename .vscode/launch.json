  "version": "0.2.0",
  "configurations": [
    {
      "name": "finance_app",
      "request": "launch",
      "type": "dart",
      "program": "lib/main_dev.dart",
      "args": [
        "--flavor",
        "dev",
        "--target",
        "lib/main_dev.dart"
      ]
    },
    {
      "name": "finance_app (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile",
      "program": "lib/main_dev.dart",
      "args": [
        "--flavor",
        "dev",
        "--target",
        "lib/main.dart"
      ]
    },
    {
      "name": "finance_app (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release",
      "program": "lib/main_prod.dart",
      "args": [
        "--flavor",
        "prod",
        "--target",
        "lib/main.dart"
      ]
    }
  ]
}